#### EUR Stoxx VIX ####
eur_stoxx_vix = pd.read_csv('/Users/nadjos/Desktop/Uni/speciale/data/eur_stoxx_vix.csv', delimiter=';', header=0, names=['date','ticker', 'value'])
# Convert the 'Date' column to datetime
eur_stoxx_vix['date'] = pd.to_datetime(eur_stoxx_vix['date'], dayfirst= True )
# Interpolate missing values
eur_stoxx_vix['value'].interpolate(method='linear', inplace=True)
# Filter the data to start from 01/01/2004
eur_stoxx_vix = eur_stoxx_vix[eur_stoxx_vix['date'] >= '2005-01-01']
eur_stoxx_vix = eur_stoxx_vix[eur_stoxx_vix['date'] <= '2024-01-01']
#length
print(len(eur_stoxx_vix['value']))

#### EUR Stoxx ####
eur_stoxx = pd.read_csv('/Users/nadjos/Desktop/Uni/speciale/data/eur_stoxx.csv', header=0, names=['date', 'value'])
# Convert the 'Date' column to datetime
eur_stoxx['date'] = pd.to_datetime(eur_stoxx['date'].apply(lambda x: x[:10]), dayfirst= True)
# Interpolate missing values
eur_stoxx['value'].interpolate(method='linear', inplace=True)
# Filter the data to start from 01/01/2004
eur_stoxx = eur_stoxx[eur_stoxx['date'] >= '2005-01-01']
eur_stoxx = eur_stoxx[eur_stoxx['date'] <= '2024-01-01']
#length
print(len(eur_stoxx['value']))

