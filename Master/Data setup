#### US S&P 500 ####
us_sp500 = pd.read_csv('/Users/nadjos/Desktop/Uni/speciale/data/us_sp500_filtered.csv', header=0, names=['date','open', 'high','low', 'close', 'volume'])
# Convert the 'Date' column to datetime
us_sp500['date'] = pd.to_datetime(us_sp500['date'])
# Interpolate missing values
us_sp500['close'].interpolate(method='linear', inplace=True)
# Filter the data to start from 01/01/2004
us_sp500 = us_sp500[us_sp500['date'] >= '2005-01-01']
us_sp500 = us_sp500[us_sp500['date'] <= '2024-01-01']
#length
print(len(us_sp500['close']))

#### US S&P 500 VIX ####
us_sp500_vix = pd.read_csv('/Users/nadjos/Desktop/Uni/speciale/data/us_sp_500_vix.csv', header=0, names=['date','open', 'high','low', 'close'], na_values='.')
# Convert the 'Date' column to datetime
us_sp500_vix['date'] = pd.to_datetime(us_sp500_vix['date'])
# Replace "." with NaN
us_sp500_vix['close'].replace(".", np.nan, inplace=True)
# Interpolate missing values
us_sp500_vix['close'].interpolate(method='linear', inplace=True)
# Filter the data to start from 01/01/2004
us_sp500_vix = us_sp500_vix[us_sp500_vix['date'] >= '2005-01-01']
us_sp500_vix = us_sp500_vix[us_sp500_vix['date'] <= '2024-01-01']
#length
print(len(us_sp500_vix['close']))

