import pandas as pd
import numpy as np

#### Brent ####
brent = pd.read_csv('/Users/nadjos/Desktop/Uni/speciale/data/brent_filtered.csv')
# Convert the 'Date' column to datetime
brent['date'] = pd.to_datetime(brent['date'])
# Interpolate missing values
brent['value'].interpolate(method='linear', inplace=True)
# Filter the data to start from 01/01/2004
brent = brent[brent['date'] >= '2005-01-01']
brent = brent[brent['date'] <= '2024-01-01']
#length
print(len(brent['value']))

#### EU CPI ####
eu_cpi = pd.read_csv('/Users/nadjos/Desktop/Uni/speciale/data/eu_cpi.csv', delimiter=';', header=0, names=['date', 'value'])
# Convert the 'Date' column to datetime
eu_cpi['date'] = pd.to_datetime(eu_cpi['date'])
# Interpolate missing values
eu_cpi['value'].interpolate(method='linear', inplace=True)
# Filter the data to start from 01/01/2004
eu_cpi = eu_cpi[eu_cpi['date'] >= '2005-01-01']
eu_cpi = eu_cpi[eu_cpi['date'] <= '2024-01-01']
#length
print(len(eu_cpi['value']))

